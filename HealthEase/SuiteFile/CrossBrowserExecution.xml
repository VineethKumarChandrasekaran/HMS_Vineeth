<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="5" parallel="tests" name="Cross Browser Suite">
	<listeners>
		<listener class-name="com.hms.generic.listenerutilities.ListenerUtility"></listener>
	</listeners>
	<test name="Chrome Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.hms.base.admin.AdminModuleTest" />
		</classes>
	</test> <!--
	Test -->
	<test name="FireFox Test">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.hms.base.admin.AdminModuleTest" />
		</classes>
	</test> <!--
	Test -->
	<test name="Edge Test">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.hms.base.admin.AdminModuleTest" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
